 <tr class="patient_charge">
   <% if charges %> 
    <td class="patient_name" patientId="<%=charges.id%>"> <%= charges.patient_name %> </td>
    <td class="procedure_name"><%= charges.procedure_name %> </td>
  <!--  <td class="procedure_code">charges.procedure_code %> </td>    -->
    <td class="procedure_notes">
           <table>
             <tr>
             <% if charges %>
              <td><%= charges.option %></td>
              <td><%= charges.procedure_type %></td> 
              <td><%= charges.locality %></td>
              
             </tr>
             <!-- <tr>
                            <td><%= charges.locality %></td> -->
             <%end%>
           </tr>
          </table>
     </td>

    <td class="doctor"><%= charges.doctor %> </td>
    <td class="fin"><%= charges.fin %> </td>  
    <td class="created_at"><%= charges.created_at.strftime("%m/%d/%Y  %I:%M%p")  %></td>
    <td class="recorded"><%= charge_recorded?(charges.recorded) %></td>
    
    <td>
      <table id="inner_table" border="0" cellspacing="0">
        <tr>
        <td class="patient_button"><input type="button" class="record_charge_button" value="record"/></td>
        </tr>
      </table>
    </td>  
    <%end%>
  </tr>


