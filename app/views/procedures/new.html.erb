<table class='stripeMe'>
	<caption>New Procedure</caption>
<tbody>
  <% form_for(@procedure) do |f| %>
    <%= f.error_messages %>
    <tr>
      <td>Procedure Name</td>
      <td><%= f.text_field :procedure_name %></td>
    </tr>
    <tr>
      <td>Procedure Code</td>
      <td><%= f.text_field :procedure_code %></td>
    </tr>
    <tr>
      <td>Procedure Nickname</td>
      <td><%= f.text_field :procedure_nickname %></td>
    </tr>
    <tr>
      <td>Group Type</td>
      <td><%= f.text_field :group_type %></td>    
    </tr>
    <tr>
      <td>Requires Note</td>
      <td><%= f.check_box :note_required,:disabled => true, :readonly => true  %></td>    
    </tr>
    
    <tr>
      <td><%= link_to 'Back', procedures_path %></td>
      <td><%= f.submit 'Create' %></td
    </tr>
    <% end %>
</tbody>

</table>