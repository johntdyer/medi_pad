<h1>New charge</h1>

<% form_for(@charge) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :procedure_name %><br />
    <%= f.text_field :procedure_name %>
  </p>
  <p>
    <%= f.label :procedure_code %><br />
    <%= f.text_field :procedure_code %>
  </p>
  <p>
    <%= f.label :doctor %><br />
    <%= f.text_field :doctor %>
  </p>
  <p>
    <%= f.label :patient_id %><br />
    <%= f.text_field :patient_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', charges_path %>